CREATE OR REPLACE FUNCTION tienda.f_crearpedidos(
	_referencia text,
	_talla double precision,
	_cantidad integer,
	_idpedido integer)
    RETURNS SETOF void 
    LANGUAGE 'plpgsql'

    COST 100
    VOLATILE 
    ROWS 1000
AS $BODY$

BEGIN
INSERT INTO tienda.pedidos(
	referencia, 
	talla, 
	cantidad, 
	idpedido)
	VALUES (
	_referencia, 
	_talla, 
	_cantidad, 
	_idpedido);
END

$BODY$;