CREATE OR REPLACE FUNCTION tienda.f_verinventario(
	)
    RETURNS SETOF tienda.v_inventario 
    LANGUAGE 'plpgsql'

    COST 100
    VOLATILE 
    ROWS 1000
AS $BODY$

BEGIN
RETURN QUERY
SELECT

uu.idproducto AS idproducto,
uu.referencia AS referencia,
uu.talla AS talla,
uu.cantidad AS cantidad,
uu.sede

FROM tienda.inventario uu;
END;

$BODY$;